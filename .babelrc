{
  "plugins": [
    [
     "transform-assets-import-to-string",
     {
       "baseDir": "static",
       "baseUri": "/"
     }
    ],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles",
          "components": "./components",
          "reducers": "./reducers",
          "actions": "./actions",
          "utils": "./utils",
          "images": "./static/images",
          "offline": "./offline"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": []
}
